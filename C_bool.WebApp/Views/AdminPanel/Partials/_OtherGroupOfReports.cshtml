@using Microsoft.AspNetCore.Html
@using Newtonsoft.Json
@model C_bool.WebApp.Models.Reports.ReportViewModel

<div class="col">
<div class="card border-0 shadow">
    <div class="card-body">
        
                <!-- List Item 1 -->
                <div class="list-group-item border-0">
                    <div class="row ps-lg-1">
                        <div class="col-auto">
                            <div class="icon-shape icon-xs icon-shape-purple rounded">
                                <svg class="icon" fill="currentColor" viewBox="0 0 22 22" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3.055 11H5a2 2 0 012 2v1a2 2 0 002 2 2 2 0 012 2v2.945M8 3.935V5.5A2.5 2.5 0 0010.5 8h.5a2 2 0 012 2 2 2 0 104 0 2 2 0 012-2h1.064M15 20.488V18a2 2 0 012-2h3.064M21 12a9 9 0 11-18 0 9 9 0 0118 0z"></path></svg>
                            </div>
                        </div>
                        <div class="col ms-n2 mb-1">
                            <h3 class="fs-6 fw-bold mb-1">Liczba miejsc bez zadań</h3>
                            <p class="mb-1">@Model.PlaceWithoutGameTasksCount.PlacesCountWithoutGameTask</p>
                        </div>
                    </div>
                </div>
                <!-- List Item 2 -->
                <div class="list-group-item border-0">
                    <div class="row ps-lg-1">
                        <div class="col-auto">
                            <div class="icon-shape icon-xs icon-shape-purple rounded">
                                <svg class="icon" fill="currentColor" viewBox="0 0 22 22" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3.055 11H5a2 2 0 012 2v1a2 2 0 002 2 2 2 0 012 2v2.945M8 3.935V5.5A2.5 2.5 0 0010.5 8h.5a2 2 0 012 2 2 2 0 104 0 2 2 0 012-2h1.064M15 20.488V18a2 2 0 012-2h3.064M21 12a9 9 0 11-18 0 9 9 0 0118 0z"></path></svg>
                            </div>
                        </div>
                        <div class="col ms-n2 mb-1">
                            <h3 class="fs-6 fw-bold mb-1">Liczba wszystkich miejsc</h3>
                            <p class="mb-1">@Model.PlaceWithoutGameTasksCount.TotalPlacesCount</p>
                        </div>
                    </div>
                </div>
                <!-- List Item 3 -->
                <div class="list-group-item border-0">
                    <div class="row ps-lg-1">
                        <div class="col-auto">
                            <div class="icon-shape icon-xs icon-shape-success rounded">
                                <svg class="icon" fill="currentColor" viewBox="0 0 22 22" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 18.657A8 8 0 016.343 7.343S7 9 9 10c0-2 .5-5 2.986-7C14 5 16.09 5.777 17.656 7.343A7.975 7.975 0 0120 13a7.975 7.975 0 01-2.343 5.657z"></path><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9.879 16.121A3 3 0 1012.015 11L11 14H9c0 .768.293 1.536.879 2.121z"></path></svg>                            </div>
                        </div>
                        <div class="col ms-n2 mb-1">
                            <h3 class="fs-6 fw-bold mb-1">Liczba ukończonych zadań</h3>
                            <p class="mb-0">@Model.UserGameTaskDoneCount.DoneCount</p>
                        </div>
                    </div>
                </div>
                <!-- List Item 4 -->
                <div class="list-group-item border-0">
                    <div class="row ps-lg-1">
                        <div class="col-auto">
                            <div class="icon-shape icon-xs icon-shape-warning rounded">
                                <svg class="icon" fill="currentColor" viewBox="0 0 22 22" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 18.657A8 8 0 016.343 7.343S7 9 9 10c0-2 .5-5 2.986-7C14 5 16.09 5.777 17.656 7.343A7.975 7.975 0 0120 13a7.975 7.975 0 01-2.343 5.657z"></path><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9.879 16.121A3 3 0 1012.015 11L11 14H9c0 .768.293 1.536.879 2.121z"></path></svg>
                            </div>
                        </div>
                        <div class="col ms-n2 mb-1">
                            <h3 class="fs-6 fw-bold mb-1">Liczba wszystkich zadań</h3>
                            <p class="mb-0">@Model.UserGameTaskDoneCount.TotalCount</p>
                        </div>
                    </div>
                </div>
                <!-- List Item 4 -->
                <div class="list-group-item border-0">
                    <div class="row ps-lg-1">
                        <div class="col-auto">
                            <div class="icon-shape icon-xs icon-shape-purple rounded">
                                <svg class="icon" fill="currentColor" viewBox="0 0 22 22" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 4.354a4 4 0 110 5.292M15 21H3v-1a6 6 0 0112 0v1zm0 0h6v-1a6 6 0 00-9-5.197M13 7a4 4 0 11-8 0 4 4 0 018 0z"></path></svg>
                            </div>
                        </div>
                        <div class="col ms-n2 mb-1">
                            <h3 class="fs-6 fw-bold mb-1">Liczba aktywnych użytkowników</h3>
                            <p class="mb-0">@Model.ActiveUsersCount.ActiveCount</p>
                        </div>
                    </div>
                </div>
                <!-- List Item 5 -->
                <div class="list-group-item border-0">
                    <div class="row ps-lg-1">
                        <div class="col-auto">
                            <div class="icon-shape icon-xs icon-shape-purple rounded">
                                <svg class="icon" fill="currentColor" viewBox="0 0 22 22" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 4.354a4 4 0 110 5.292M15 21H3v-1a6 6 0 0112 0v1zm0 0h6v-1a6 6 0 00-9-5.197M13 7a4 4 0 11-8 0 4 4 0 018 0z"></path></svg>
                            </div>
                        </div>
                        <div class="col ms-n2 mb-1">
                            <h3 class="fs-6 fw-bold mb-1">Liczba wszystkich użytkowników</h3>
                            <p class="mb-0">@Model.ActiveUsersCount.TotalCount</p>
                        </div>
                    </div>
                </div>
    </div>
</div>
</div>
